<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.ecoworker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ecoworker"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".mypage.MypageActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />

        <activity android:name=".stat.StatActivity" android:label = "Stat 액티비티">
        </activity>
        <activity android:name=".stat.DateNewActivity" android:label = "DateNew 액티비티">
        </activity>
        <activity android:name=".stat.DateModActivity" android:label = "DateMod 액티비티">
        </activity>

    </application>

</manifest>